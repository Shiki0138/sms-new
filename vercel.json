{
  "regions": ["hnd1"],
  "buildCommand": "echo 'No build required'",
  "devCommand": "PORT=3000 node app/backend/server-clean.js",
  "installCommand": "npm install",
  "framework": null,
  "outputDirectory": "app/frontend",
  "public": false,
  "headers": [],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/",
      "destination": "/login.html"
    },
    {
      "source": "/login",
      "destination": "/login.html"
    },
    {
      "source": "/dashboard",
      "destination": "/dashboard.html"
    },
    {
      "source": "/customers",
      "destination": "/customers.html"
    },
    {
      "source": "/appointments",
      "destination": "/appointments.html"
    },
    {
      "source": "/staff",
      "destination": "/staff.html"
    },
    {
      "source": "/services",
      "destination": "/services.html"
    },
    {
      "source": "/reports",
      "destination": "/reports.html"
    },
    {
      "source": "/messages",
      "destination": "/messages.html"
    },
    {
      "source": "/customer-edit",
      "destination": "/customer-edit.html"
    },
    {
      "source": "/customer-detail",
      "destination": "/customer-detail.html"
    }
  ]
}